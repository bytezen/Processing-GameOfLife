<?xml version="1.0"?>
<config>
	<!--	APP config	-->	
	<param>
		<!-- rows and columns of grid -->
		<key>gridMatrixSize</key> 
		<value>100,100</value>
	</param>
	<param>
		<!-- grid in pixels -->
		<key>gridSize</key>
		<value>1024,1024</value>
	</param>
	<param>
		<!-- number between 1 - 30; 30 is render once per second -->
		<key>renderSpeed</key>
		<value>30</value>
	</param>
	<!--	LEMUR config	-->
	<param>
		<key>lemurPadButtonPattern</key>
		<value>\\/CustomButton(\\d+)\\/x</value>
	</param>
	<param>
		<key>lemurControlButtonPattern</key>
		<value>"\\/Control\\/play"</value>
	</param>	
</config>