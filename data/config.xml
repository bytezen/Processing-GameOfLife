<?xml version="1.0"?>
<config>
	<!--	APP config	-->	
	<param>
		<!-- rows and columns of grid -->
		<key>gridMatrixSize</key> 
		<value>100,100</value>
	</param>
	<param>
		<!-- grid in pixels -->
		<key>gridSize</key>
		<value>1024,1024</value>
	</param>
	<param>
		<!-- number between 1 - 30; 30 is render once per second -->
		<key>renderSpeed</key>
		<value>5</value>
	</param>
	<param>
		<key>cellShape</key>
		<value>rectangle</value> <!-- rectangle or ellipse -->
	</param>	
	<!--	LEMUR config	-->
	<param>
		<key>lemurControlButtonPattern</key>
		<value>"\\/Control\\/play"</value>
	</param>		
	<param>
		<key>lemurPadAddr</key>
		<value>/Pixels/x</value>
	</param>
	<param>
		<key>lemurRows,lemurCols</key>
		<value>9,16</value>
	</param>	
</config>